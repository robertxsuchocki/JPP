[int] forty_two_first([int] list) {
  list[0] = 42;
  return list;
}
int main () {
  [int] x;
  x = [];
  for i = 0..8 do {
    x[i] = i;
  }
  x[9] = 10;
  if (length(x) == x[9]) {
    x = forty_two_first(x);
    if (x[0] == 42) {
      println("success!");
      # prints success!
      return 0;
    }
  }
  x[42] = 0;
  # this would produce an error
  return 0;
}
