[[int]] forty_two_all_in_first([[int]] list) {
  [int] tmp = list[0];
  int len = length(tmp);
  for i = 0..len-1 do {
    tmp[i] = 42;
  }
  list[0] = tmp;
  return list;
}
int main () {
  [[int]] x = [];
  [int] tmp;

  for i = 0..10 do {
    tmp = [];
    for i = 0..10 do {
      tmp[i] = i;
    }
    x[i] = tmp;
  }
  x = forty_two_all_in_first(x);
  tmp = x[0];
  if (tmp[7] == 42) {
    println("success!");
    # prints success!
  }
  return 0;
}
