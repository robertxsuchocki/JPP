int main () {
  int x = 1000;
  int y = 10;
  void a(int x) {
    y = y + x;
  }
  void b(int x) {
    y = y - x;
  }
  int fac(int x) {
    if (x <= 1) {
      return 1;
    } else {
      return x * fac(x-1);
    }
  }
  for i = 1..10 do {
    if (x != 0) {
      a(-1);
    } else {
      b(1);
    }
    x = x + y;
  }
  println(x);
  println(fac(10));
  println(toInt("40") + toInt("2"));
}
